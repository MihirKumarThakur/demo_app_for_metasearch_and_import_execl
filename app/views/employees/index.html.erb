<h1>Employee</h1>


<%= form_for @search, :url => articles_path, :html => {:method => :get} do |f| %>
  <%= f.label :title_contains %>
  <%= f.text_field :title_contains %><br />
  <%= f.label :comments_created_at_greater_than, 'With comments after' %>
  <%= f.datetime_select :comments_created_at_greater_than, :include_blank => true %><br />
  <!-- etc... -->
  <%= f.submit %>
<% end %>



<table id="Employee">
  <tr>
    <th>department_id</th>
    <th>ssn</th>
    <th>name</th>
    <th>phone_number</th>
    <th>address</th>
    <th>salary</th>
  </tr>
<% @employees.each do |employee| %>
  <tr>
    <td><%= employee.department_id %></td>
    <td><%= employee.ssn %></td>
    <td><%= employee.name %></td>
    <td><%= employee.phone_number %></td>
     <td><%= employee.address %></td>
      <td><%= employee.salary  %></td>
  </tr>
<% end %>
</table>

<p><%= link_to "Import employees", new_employee_import_path %></p>

